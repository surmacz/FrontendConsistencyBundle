{
    "name": "surmacz/frontend-consistency-bundle",
    "type": "symfony-bundle",
    "description": "This Bundle provides system to help save frontend consistency on different web browsers.",
    "keywords": ["frontend", "consistency"],
    "homepage": "",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Wojciech Surmacz",
            "email": "wojciech.surmacz@gmail.com"
        }
    ],
    "archive": {
        "exclude": ["/Tests"]
    },
    "require": {
        "php": ">=5.4.0",
        "symfony/finder": "~2.3",
        "symfony/filesystem": "~2.3",
        "symfony/console": "~2.3",
        "symfony/config": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/http-kernel": "~2.3",
        "symfony/process": "~2.3",
        "symfony/framework-bundle": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-0": { "Surmacz\\FrontendConsistencyBundle": "" }
    },
    "target-dir": "Surmacz/FrontendConsistencyBundle",
    "extra" : {
        "branch-alias" : {
            "dev-master" : "0.0.1"
        }
    }
}